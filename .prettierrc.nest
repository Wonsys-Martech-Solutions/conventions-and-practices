{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "printWidth": 150,
    "semi": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "importOrderParserPlugins": ["typescript", "decorators-legacy"],
    "importOrder": ["^@nestjs", "^@?\\w", "^@/", "^[./]", "^../../", "^[../]"],
    "importOrderSortSpecifiers": true
}
