{
    "singleQuote": true,
    "useTabs": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "printWidth": 150,
    "semi": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "importOrder": ["^@nestjs", "^@?\\w", "^[./]", "^../../", "^[../]", "^@/"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
